const displayModal = (modal, matchid) => {
  console.log(matchid);
  const potentialmatch = document.querySelector("#potentialMatches");
  potentialmatch.insertAdjacentHTML('beforeend', modal);
  $(`#matchModal-${matchid}`).modal();
};

<% if @match %>
  displayModal('<%= j render "matches/matchmodal", match: @match, item: @my_item %>', '<%= @match.id %>');
  <% else %>
  window.location.replace("<%= potential_matches_item_path(@my_item) %>");
<% end %>


