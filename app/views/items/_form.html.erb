<%= simple_form_for @item do |f| %>
    <%= f.input :name %>
    <%= f.input :brand %>
    <%= f.input :description, as: :text, :input_html => { :cols => 5, :rows => 5 }%>
    <%= f.input :min_price %>
    <%= f.input :max_price %>
    <%= f.input :picture, as: :file %>
    <%= f.submit %>
  <% end %>
